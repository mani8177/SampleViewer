<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mani8177.sampleviewer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.mani8177.sampleviewer.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mani8177.sampleviewer.SampleActivity"
            android:label="@string/title_activity_sample"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.example.mani8177.sampleviewer.TabSampleActivity"
            android:label="@string/title_activity_tab_sample"
            android:theme="@style/myTheme">
        </activity>
    </application>

</manifest>
